<UserControl x:Class="morphological_image_processing_wpf.View.Components.AlgorithmsSelection.Configurations.AlgorithmSelectionComponent"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:morphological_image_processing_wpf.View.Components.AlgorithmsSelection.Configurations"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="200">
    <Grid Background="White">
        <Label Content="Select Algorithm" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Width="180"/>
        <ComboBox x:Name="AlgorithmComboBox" HorizontalAlignment="Center" Margin="0,36,0,0" VerticalAlignment="Top" Width="180" Height="28" SelectionChanged="AlgorithmComboBox_SelectionChanged"/>
        <local:BaseAlgorithmConfigurationComponent x:Name="baseAlgorithmConfigurationComponent"  HorizontalAlignment="Center" Height="331" Margin="0,69,0,0" VerticalAlignment="Top" Width="180"/>
    </Grid>
</UserControl>
